{
    "name": "grosv/eloquent-sheets",
    "description": "Eloquent models backed by Google Sheets",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ed Grosvenor",
            "email": "ed.grosvenor@wickedviral.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "illuminate/support": ">=6.0",
        "revolution/laravel-google-sheets": "^5.0",
        "calebporzio/sushi": "^2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Grosv\\EloquentSheets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": "Grosv\\EloquentSheets\\EloquentSheetsProvider"
        },
        "branch-alias": {
            "dev-master": "1.3.0-dev"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix -v",
        "test:unit": "phpunit",
        "test": [
            "@lint",
            "@test:unit"
        ]
    }
}
